.page, .bestme-pages {
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.bestme-pages{
  padding: 0;
  margin: 0;
}

.blur {
   //filter: url(blur.svg#blur);
   -webkit-filter: blur(3px) brightness(1);
   -moz-filter: blur(3px) brightness(1);
   //filter: progid:DXImageTransform.Microsoft.Blur(PixelRadius=10, MakeShadow=false); /* IE6~IE9 */
}
.bg {
   /*http://bpic.588ku.com/back_pic/04/57/95/355867c7841af40.jpg!/fw/650/quality/90/unsharp/true/compress/true')*/
   background: #ede3c5 ;//url(/images/bestme/bg-home.jpg) center;
   background-size: cover;
   filter: blur(9px);
   height: 110%;
   width: 110%;
   position: absolute;
   z-index: 0;
   margin: -5%;
}
.content {
   position: absolute;
   z-index: 1;
   top: 0;
   bottom: 0;
   left: 0;
   right: 0;
   margin: 0 auto;
   padding: 15px;
   border-radius: 5px;
   color: #333;
   border-color: rgba(250, 250, 250, 0.5);
   overflow: auto;
   -webkit-overflow-scrolling : touch;
}

.swiper-container{
   height: 100%;
}
.parallax-bg {
   background-color: #ede3c5;
   position: absolute;
   left: 0;
   top: 0;
   width: 130%;
   height: 100%;
   -webkit-background-size: cover;
   background-size: cover;
   background-position: center;
}
.parallax-warpper {
   width: 100%;
   height: 100%;
}

@-webkit-keyframes breathe {
  0% {
    opacity: 0.8;
    border-color: rgba(166, 213, 24, 0.6);
    box-shadow: 0 1px 30px rgba(187, 187, 187, 1);
  }
  100% {
    opacity: 1;
    border-color: rgba(166, 213, 24, 0.1);
    box-shadow: 0 1px 2px rgba(255, 255, 255, 0.1);
  }
}

@import "home.styl";
@import "photo.styl";
@import "transition.styl";
@import "music.styl";
@import "others.styl";