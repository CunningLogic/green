.others-page{
  /* padding: 30px 15px;*/
  line-height: 24px;
  /*background: rgba(43, 218, 217, 0.8);*/
  background: #a19b51;
  color: #fff;

  .top-list{
    position: absolute;
    z-index: 3;
  }
  .top-item{
    position: absolute;
    height: 100%;
    width: 100%;
    overflow: hidden;
  }

  .bg-wrap{
    position:absolute;
    z-index: 1;
    height: 100%;
    width: 100%;
    overflow: hidden;
  }
  .canvas-bg{
    height: 110%;
    width: 110%;
  }
  .top-item .content{
    position:absolute;
    z-index: 3;
    width: 100%;
    height: 100%;
  }

  .title{
    text-align: center;
    margin-bottom: 30px;
    line-height: 25px;
    color: #fff;
    padding:20px 20px 5px 20px;
  }
  .title .logo{
    color: #d4b050;
    font-weight: bold;
  }

  .picture{
    min-height: 300px;
    width: 200px;
    margin: auto;
  }
  .desc{
    position: absolute;
    bottom: 0;
    left:0;
    padding: 15px;
    color: #fff;
    background: rgba(0, 0, 0, 0.3);
    width: 100%;
  }
}