.dui-notice{
  color: #c7254e;
  margin-bottom:8px !important;
}
.dui-tpl{
  display:none !important;
}

#mask{
  .load{
    position: fixed;
    top: 50%;
    left: 50%;
    margin-left:-16px;
    margin-top:-16px;
    width: 32px;
    height: 32px;
    z-index: 999999;
    .wait-msg{
      width: 320px;
      margin-left: -145px;
      text-align: center;
      color: #999;
    }
  }
  .overlay{
    position: fixed;
    width: 100%;
    height: 100%;
    background: #f4f4f4;
    top: 0;
    opacity: 0.7;
    z-index: 999998;
  }
}
